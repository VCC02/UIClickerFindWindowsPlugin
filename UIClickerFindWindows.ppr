procedure ConcatTIntArr(var ADest, ASrc: TIntArr);
var
  i: Integer;
begin
  for i := 0 to Length(ASrc) - 1 do
  begin
    SetLength(ADest, Length(ADest) + 1);  //it would be faster to preallocate
    ADest[Length(ADest) - 1] := ASrc[i];
  end;
end;


//'WinLeft_0=30' + #4#5 + 'WinTop_0=90' + #4#5 + 'WinRight_0=250' + #4#5 + 'WinBottom_0=400' + #4#5;  //this example may change
procedure DecodeWindowsByEdges(var AllFindSubControlExecutionResults: TStringArr; var ExistingCorners, WindowFeaturesArr: TWindowFeaturesArr);
var
  AllVars, XOffsets, YOffsets, MatchSource: TStringList;
  i, j, EdgePropIndex, ExistingCornerIndex: Integer;
  CornerTypeCount: Integer;
  CornerIndex: Integer;
  AllControl_XOffsets, AllControl_YOffsets, AllControl_MatchSource: string;
  ValidWindowFeaturesArr: TWindowFeaturesArr;

  //These are edges, not window corners:
  TopLeftCorner_XOffsetsIntArr, TopLeftCorner_YOffsetsIntArr, TopLeftCorner_SrcIdx: TIntArr;
  BotLeftCorner_XOffsetsIntArr, BotLeftCorner_YOffsetsIntArr, BotLeftCorner_SrcIdx: TIntArr;
  TopRightCorner_XOffsetsIntArr, TopRightCorner_YOffsetsIntArr, TopRightCorner_SrcIdx: TIntArr;
  BotRightCorner_XOffsetsIntArr, BotRightCorner_YOffsetsIntArr, BotRightCorner_SrcIdx: TIntArr;

  Temp_TopLeftCorner_XOffsetsIntArr, Temp_TopLeftCorner_YOffsetsIntArr, Temp_TopLeftCorner_SrcIdx: TIntArr;
  Temp_BotLeftCorner_XOffsetsIntArr, Temp_BotLeftCorner_YOffsetsIntArr, Temp_BotLeftCorner_SrcIdx: TIntArr;
  Temp_TopRightCorner_XOffsetsIntArr, Temp_TopRightCorner_YOffsetsIntArr, Temp_TopRightCorner_SrcIdx: TIntArr;
  Temp_BotRightCorner_XOffsetsIntArr, Temp_BotRightCorner_YOffsetsIntArr, Temp_BotRightCorner_SrcIdx: TIntArr;

  //DbgStr: string;
begin
  CornerTypeCount := Length(ExistingCorners);

  for j := 0 to Length(AllFindSubControlExecutionResults) - 1 do
  begin
    AllVars := TStringList.Create;
    try
      AllVars.Text := AllFindSubControlExecutionResults[j];

      AllControl_XOffsets := AllVars.Values['$AllControl_XOffsets$'];       //example: 19460544
      AllControl_YOffsets := AllVars.Values['$AllControl_YOffsets$'];       //example: 244188428
      AllControl_MatchSource := AllVars.Values['$AllControl_MatchSource$']; //example: pmtv[0]pmtv[1]pmtv[1]
                                                                            //meaning: one active window, then two inactive windows
      XOffsets := TStringList.Create;
      YOffsets := TStringList.Create;
      MatchSource := TStringList.Create;
      try
        XOffsets.Text := FastReplace_45ToReturn(AllControl_XOffsets);
        YOffsets.Text := FastReplace_45ToReturn(AllControl_YOffsets);
        MatchSource.Text := FastReplace_45ToReturn(AllControl_MatchSource);

        EdgePropIndex := StrToIntDef(AllVars.Values[CActionPlugin_EdgePropIndexVar], -1);  //range 4..9
        ExistingCornerIndex := StrToIntDef(AllVars.Values[CActionPlugin_WindowFeatureIndexVar], 0);  //range 0..CornerTypeCount- 1     (i.e. CornerTypeCount = <number of primitive files>)

        case EdgePropIndex of
          CFindSubControlLeftEdge_PropIndex:
            SetCornerAsIntArr(XOffsets, YOffsets, MatchSource, Temp_TopLeftCorner_XOffsetsIntArr, Temp_TopLeftCorner_YOffsetsIntArr, Temp_TopLeftCorner_SrcIdx);

          CFindSubControlTopEdge_PropIndex:
            SetCornerAsIntArr(XOffsets, YOffsets, MatchSource, Temp_BotLeftCorner_XOffsetsIntArr, Temp_BotLeftCorner_YOffsetsIntArr, Temp_BotLeftCorner_SrcIdx);

          CFindSubControlRightEdge_PropIndex:
            SetCornerAsIntArr(XOffsets, YOffsets, MatchSource, Temp_TopRightCorner_XOffsetsIntArr, Temp_TopRightCorner_YOffsetsIntArr, Temp_TopRightCorner_SrcIdx);

          CFindSubControlBottomEdge_PropIndex:
            SetCornerAsIntArr(XOffsets, YOffsets, MatchSource, Temp_BotRightCorner_XOffsetsIntArr, Temp_BotRightCorner_YOffsetsIntArr, Temp_BotRightCorner_SrcIdx);
        end; //case
      finally
        XOffsets.Free;
        YOffsets.Free;
        MatchSource.Free;
      end;
    finally
      AllVars.Free;
    end;

    ConcatTIntArr(TopLeftCorner_XOffsetsIntArr, Temp_TopLeftCorner_XOffsetsIntArr);
    ConcatTIntArr(BotLeftCorner_XOffsetsIntArr, Temp_BotLeftCorner_XOffsetsIntArr);
    ConcatTIntArr(TopRightCorner_XOffsetsIntArr, Temp_TopRightCorner_XOffsetsIntArr);
    ConcatTIntArr(BotRightCorner_XOffsetsIntArr, Temp_BotRightCorner_XOffsetsIntArr);

    ConcatTIntArr(TopLeftCorner_YOffsetsIntArr, Temp_TopLeftCorner_YOffsetsIntArr);
    ConcatTIntArr(BotLeftCorner_YOffsetsIntArr, Temp_BotLeftCorner_YOffsetsIntArr);
    ConcatTIntArr(TopRightCorner_YOffsetsIntArr, Temp_TopRightCorner_YOffsetsIntArr);
    ConcatTIntArr(BotRightCorner_YOffsetsIntArr, Temp_BotRightCorner_YOffsetsIntArr);

    ConcatTIntArr(TopLeftCorner_SrcIdx, Temp_TopLeftCorner_SrcIdx);
    ConcatTIntArr(BotLeftCorner_SrcIdx, Temp_BotLeftCorner_SrcIdx);
    ConcatTIntArr(TopRightCorner_SrcIdx, Temp_TopRightCorner_SrcIdx);
    ConcatTIntArr(BotRightCorner_SrcIdx, Temp_BotRightCorner_SrcIdx);
  end; //for j

  SetLength(WindowFeaturesArr, 0);

  //matching TopLeft corner with BotLeft
  for i := 0 to Length(TopLeftCorner_XOffsetsIntArr) - 1 do    //if the edge is too short, then multiple edges are found (partially overlapped)
  begin
    CornerIndex := IndexOfTopLeftCorner(WindowFeaturesArr, TopLeftCorner_XOffsetsIntArr[i], TopLeftCorner_YOffsetsIntArr[i], TopLeftCorner_SrcIdx[i], False, True);
    for j := 0 to Length(ExistingCorners) - 1 do
      if (TopLeftCorner_XOffsetsIntArr[i] = ExistingCorners[j].TopLeftCorner.X) and
         (TopLeftCorner_SrcIdx[i] = ExistingCorners[j].TopLeftCornerSrcIdx) then   //the same SrcIdx means the two corners were found with the same FindSubControl settings.
      begin
        if CornerIndex = -1 then
        begin
          SetLength(WindowFeaturesArr, Length(WindowFeaturesArr) + 1);
          InitWindowFeatures(WindowFeaturesArr[Length(WindowFeaturesArr) - 1]);

          WindowFeaturesArr[Length(WindowFeaturesArr) - 1].TopLeftCorner.X := ExistingCorners[j].TopLeftCorner.X;
          WindowFeaturesArr[Length(WindowFeaturesArr) - 1].TopLeftCorner.Y := ExistingCorners[j].TopLeftCorner.Y;
          WindowFeaturesArr[Length(WindowFeaturesArr) - 1].TopLeftCornerSrcIdx := ExistingCorners[j].TopLeftCornerSrcIdx;

          WindowFeaturesArr[Length(WindowFeaturesArr) - 1].BotLeftCorner.X := ExistingCorners[j].BotLeftCorner.X;
          WindowFeaturesArr[Length(WindowFeaturesArr) - 1].BotLeftCorner.Y := ExistingCorners[j].BotLeftCorner.Y;
          WindowFeaturesArr[Length(WindowFeaturesArr) - 1].BotLeftCornerSrcIdx := ExistingCorners[j].BotLeftCornerSrcIdx;

          Break;
        end;
      end;
  end;

  //matching TopLeft corner with TopRight
  for i := 0 to Length(TopLeftCorner_YOffsetsIntArr) - 1 do    //if the edge is too short, then multiple edges are found (partially overlapped)
  begin
    CornerIndex := IndexOfTopLeftCorner(WindowFeaturesArr, TopLeftCorner_XOffsetsIntArr[i], TopLeftCorner_YOffsetsIntArr[i], TopLeftCorner_SrcIdx[i], True, False);
    for j := 0 to Length(ExistingCorners) - 1 do
      if (TopLeftCorner_YOffsetsIntArr[i] = ExistingCorners[j].TopLeftCorner.Y) and
         (TopLeftCorner_SrcIdx[i] = ExistingCorners[j].TopLeftCornerSrcIdx) then   //the same SrcIdx means the two corners were found with the same FindSubControl settings.
      begin
        if CornerIndex = -1 then
        begin
          SetLength(WindowFeaturesArr, Length(WindowFeaturesArr) + 1);
          InitWindowFeatures(WindowFeaturesArr[Length(WindowFeaturesArr) - 1]);

          WindowFeaturesArr[Length(WindowFeaturesArr) - 1].TopLeftCorner.X := ExistingCorners[j].TopLeftCorner.X;
          WindowFeaturesArr[Length(WindowFeaturesArr) - 1].TopLeftCorner.Y := ExistingCorners[j].TopLeftCorner.Y;
          WindowFeaturesArr[Length(WindowFeaturesArr) - 1].TopLeftCornerSrcIdx := ExistingCorners[j].TopLeftCornerSrcIdx;

          WindowFeaturesArr[Length(WindowFeaturesArr) - 1].TopRightCorner.X := ExistingCorners[j].TopRightCorner.X;
          WindowFeaturesArr[Length(WindowFeaturesArr) - 1].TopRightCorner.Y := ExistingCorners[j].TopRightCorner.Y;
          WindowFeaturesArr[Length(WindowFeaturesArr) - 1].TopRightCornerSrcIdx := ExistingCorners[j].TopRightCornerSrcIdx;

          Break;
        end;
      end;
  end;
end;


procedure GetAllRequiredFindSubControlActionNames(APluginSettings: TStringList; var AAllRequiredFindSubControlNames: TStringArr);
var
  i, j: Integer;
begin
  for i := 0 to APluginSettings.Count - 1 do   //assume more properties are there, besides FindSubControl action names for corners and edges
    for j := 0 to CMaxRequiredSubControlActions - 1 do
      if APluginSettings.Names[i] = CRequiredSubControlPropertyNames[j] then
      begin
        SetLength(AAllRequiredFindSubControlNames, Length(AAllRequiredFindSubControlNames) + 1);
        AAllRequiredFindSubControlNames[Length(AAllRequiredFindSubControlNames) - 1] := APluginSettings.ValueFromIndex[i];
        Break;
      end;
end;


function IsValidCornerPair(AListOfAllVars: TStringList; ACornerCoord, ACornerIndex: Integer; AVarName: string): Boolean; forward;


procedure HandleOnFileContent(ACallReference, AStreamContent: Pointer; AStreamSize: Int64); cdecl;
var
  TempStream: TMemoryStream;
  DestBmp: TBitmap;
begin
  if AStreamSize <= 0 then
    Exit;

  DestBmp := TBitmap(ACallReference); //created by caller

  TempStream := TMemoryStream.Create;
  try
    TempStream.SetSize(AStreamSize);
    Move(AStreamContent^, TempStream.Memory^, AStreamSize);
    DestBmp.LoadFromStream(TempStream, AStreamSize);
  finally
    TempStream.Free;
  end;
end;


