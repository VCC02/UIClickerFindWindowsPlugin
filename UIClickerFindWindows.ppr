    CornerIndex := IndexOfBotLeftCorner(WindowFeaturesArr, BotLeftCorner_XOffsetsIntArr[i], BotLeftCorner_YOffsetsIntArr[i], BotLeftCorner_SrcIdx[i]);
    for j := 0 to Length(BotRightCorner_YOffsetsIntArr) - 1 do
