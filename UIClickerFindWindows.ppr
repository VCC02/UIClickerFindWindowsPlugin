  procedure SetTemplateVar(AVarName, AVarValue: string);
  begin
    AOnActionPlugin_SetTemplateVar(APluginReference, @AVarName[1], @AVarValue[1]);
  end;

  procedure GetActionInfoByIndex(AIndex: Integer; out AActionName: string; var AActionType: DWord);
  var
    Buffer: string;
    ActionNamePtr: Pointer;
    NameLengthDWord: DWord;
  begin
    SetLength(Buffer, CMaxSharedStringLength + 1);
    ActionNamePtr := @Buffer[1];
    NameLengthDWord := 0;

    AActionName := '';
    AOnActionPlugin_GetActionInfoByIndex(APluginReference, AIndex, ActionNamePtr, @NameLengthDWord, @AActionType);
    SetPointedContentToString(ActionNamePtr, AActionName);
    SetLength(AActionName, NameLengthDWord);
  end;

  procedure GetAllTemplateVars(out AAllTemplateVars: string);  //AAllTemplateVars are encoded as CRLF separated key=value strings, ready to be used on a TStringlist
  var
    Buffer: string;
    AllTemplateVarsPtr: Pointer;
    NameLengthDWord: DWord;
  begin
    SetLength(Buffer, CMaxSharedStringLength + 1);
    AllTemplateVarsPtr := @Buffer[1];
    NameLengthDWord := 0;
    AAllTemplateVars := '';

    AOnActionPlugin_GetAllTemplateVars(APluginReference, AllTemplateVarsPtr, @NameLengthDWord);  //decode action execution results

    SetPointedContentToString(AllTemplateVarsPtr, AAllTemplateVars);
    SetLength(AAllTemplateVars, NameLengthDWord);
  end;

  procedure GetPluginSettings(out APluginSettings: string);
  begin
    APluginSettings := '';
    SetLength(PluginSettingsStr, CMaxSharedStringLength);
    SetPointedContentToString(AListOfPluginSettings, APluginSettings);
    SetLength(APluginSettings, AListOfPluginSettingsLen^);
  end;

  function BitmapIsAllowedToBeLoaded(AFnm: string): Boolean;
  var
    TempFileProvider: TFileProvider;
    TempDenyReason: string;
    FullTemplatesDir, AllowedFileDirsForServer, AllowedFileExtensionsForServer: string;
    FullTemplatesDirPtr, AllowedFileDirsForServerPtr, AllowedFileExtensionsForServerPtr: Pointer;
  begin
    TempFileProvider := TFileProvider.Create;
    try
      SetLength(FullTemplatesDir, CMaxSharedStringLength + 1);
      SetLength(AllowedFileDirsForServer, CMaxSharedStringLength + 1);
      SetLength(AllowedFileExtensionsForServer, CMaxSharedStringLength + 1);

      FullTemplatesDirPtr := @FullTemplatesDir[1];
      AllowedFileDirsForServerPtr := @AllowedFileDirsForServer[1];
      AllowedFileExtensionsForServerPtr := @AllowedFileExtensionsForServer[1];

      AOnActionPlugin_GetAllowedFilesInfo(APluginReference, FullTemplatesDirPtr, AllowedFileDirsForServerPtr, AllowedFileExtensionsForServerPtr);
      TempFileProvider.FullTemplatesDir := FullTemplatesDir;

      TempFileProvider.AddListOfAccessibleDirs(AllowedFileDirsForServer);
      TempFileProvider.AddListOfAccessibleFileExtensions(AllowedFileExtensionsForServer);

      Result := TempFileProvider.FileIsAllowed(AFnm, TempDenyReason);
    finally
      TempFileProvider.Free;
    end;
  end;

  function LoadBitmap(AFnm: string; ADestBmp: TBitmap): Boolean;
  begin
    if BitmapIsAllowedToBeLoaded(AFnm) then
      Result := AOnActionPlugin_LoadBitmap(APluginReference, @AFnm[1], ADestBmp, 0, @HandleOnFileContent)
    else
      Result := False;
  end;

  procedure GenerateSomeResultImg;
  var
    TempBmp, BkBmp: TBitmap;
    TempStream: TMemoryStream;
  begin
    TempBmp := TBitmap.Create;
    BkBmp := TBitmap.Create;
    TempStream := TMemoryStream.Create;
    try
      TempBmp.Width := 700;
      TempBmp.Height := 400;
      TempBmp.Canvas.Brush.Color := clBlack;
      TempBmp.Canvas.Pen.Color := clBlack;

      if LoadBitmap(ExtractFilePath(ParamStr(0)) + 'Tests\TestFiles\Gradient30deg.bmp', BkBmp) then
        TempBmp.Canvas.Draw(50, 25, BkBmp);

      TempBmp.Canvas.Font.Color := clYellow;
      TempBmp.Canvas.Font.Name := 'Courier New';
      TempBmp.Canvas.TextOut(30, 10, 'Plugin done.');

      TempBmp.SaveToStream(TempStream);
      AOnActionPlugin_SetResultImg(APluginReference, 0, 0, 0, TempStream.Memory, TempStream.Size, TempBmp.Width, TempBmp.Height);
    finally
      TempStream.Free;
      TempBmp.Free;
      BkBmp.Free;
    end;
  end;

  procedure SaveBitmapToExtRenderingInMemFS;
  var
    Bmp: TBitmap;
    MemStream: TMemoryStream;
  begin
    Bmp := TBitmap.Create;
    MemStream := TMemoryStream.Create;
    try
      Bmp.Width := 150;
      Bmp.Height := 180;
      Bmp.PixelFormat := pf24bit;
      Bmp.Canvas.Brush.Color := clBlack;
      Bmp.Canvas.Font.Color := clYellow;
      Bmp.Canvas.TextOut(50, 60, 'Generated bitmap');
      Bmp.Canvas.TextOut(50, 130, 'Generated bitmap');
      bmp.SaveToStream(MemStream);

      AOnActionPlugin_SetBitmap(APluginReference, @string('GenBmp.bmp')[1], MemStream.Memory, MemStream.Size, Bmp.Width, Bmp.Height);
    finally
      Bmp.Free;
      MemStream.Free;
    end;
  end;

begin
  Result := False;
  try
    DefaultOnActionPlugin_DebugPoint := AOnActionPlugin_DebugPoint;
    DefaultPluginReference := APluginReference;
    AddToLogCallback := AOnActionPlugin_AddToLog;

    if not DbgPoint('Entering ExecutePlugin', '') then     //see ClkDbgSym.ppr for expected statement formats
      Exit;

    ActionCount := AOnActionPlugin_GetActionCount(APluginReference);
    if ActionCount = 0 then
    begin
      ResName := CActionPlugin_ExecutionResultErrorVar;
      ResValue := 'This plugin does not run on an empty template.';
      AOnActionPlugin_SetTemplateVar(APluginReference, @ResName[1], @ResValue[1]);
      Exit;
    end;

    SetTemplateVar('$PluginError$', '');
    SetTemplateVar('$AllControl_XOffsets$', '');
    SetTemplateVar('$AllControl_YOffsets$', '');

    PluginSettings := TStringList.Create;
    try
      GetPluginSettings(PluginSettingsStr);
      PluginSettings.Text := PluginSettingsStr;

      if not DbgPoint('Get BorderThickness', '') then
        Exit;

      if PluginSettings.Count < CPropertiesCount then
      begin
        SetTemplateVar(CActionPlugin_ExecutionResultErrorVar, 'The plugin is configured with a wrong number of properties: ' + IntToStr(PluginSettings.Count) + ', instead of the expected, ' + IntToStr(CPropertiesCount) + '.');
        Exit;
      end;

      GetAllRequiredFindSubControlActionNames(PluginSettings, AllRequiredFindSubControlNames);
      BorderThickness := StrToIntDef(PluginSettings.ValueFromIndex[CBorderThickness_PropIndex], CDefaultBorderThickness);
      MatchWindowEdges := PluginSettings.ValueFromIndex[CMatchWindowEdges_PropIndex];

      //if not DbgPoint('After getting BorderThickness', 'Read BorderThickness: ' + IntToStr(BorderThickness)) then
      //  Exit;

      //Calling FindControl or CallTemplate, to update $Control_Left$ and $Control_Top$
      if PluginSettings.ValueFromIndex[CParentFindControl_PropIndex] <> '' then
        if not AOnActionPlugin_ExecuteAction(APluginReference, @PluginSettings.ValueFromIndex[CParentFindControl_PropIndex][1]) then  //call FindControl or CallTemplate, before each FindSubControl
        begin
          SetTemplateVar(CActionPlugin_ExecutionResultErrorVar, 'The required "' + PluginSettings.ValueFromIndex[CParentFindControl_PropIndex] + '" action has failed on updating $Control_Left$ and $Control_Top$.');
          Exit;
        end;

      //Get Control_Left and Control_Top values
      GetAllTemplateVars(AllVarsStr);

      ListOfAllVars := TStringList.Create;
      try
        ListOfAllVars.Text := AllVarsStr;
        Control_Left := StrToIntDef(ListOfAllVars.Values['$Control_Left$'], 0);
        Control_Top := StrToIntDef(ListOfAllVars.Values['$Control_Top$'], 0);
      finally
        ListOfAllVars.Free;
      end;

      //if not DbgPoint('After getting $Control_Left$ and $Control_Top$', 'Read $Control_Left$ and $Control_Top$: ' + IntToStr(Control_Left) + ' : ' + IntToStr(Control_Top)) then
      //  Exit;

      //Executing FindSubControl actions, for getting window corners
      s := 'unset action';
      for i := 0 to ActionCount - 1 do
      begin
        GetActionInfoByIndex(i, s, ActionTypeDWord);

        if ActionTypeDWord <> $FFFFFFFF then
        begin
          ActionType := TClkAction(ActionTypeDWord);

          if ActionType in [acFindSubControl, acPlugin] then
            for j := CFindSubControlTopLeftCorner_PropIndex to CFindSubControlBotRightCorner_PropIndex do  //corners:  without "- 1"
              if s = AllRequiredFindSubControlNames[j] then
              begin
                //if not DbgPoint('Calling FindControl or CallTemplate (corners)', PluginSettings.ValueFromIndex[CParentFindControl_PropIndex]) then
                //  Exit;

                if AOnActionPlugin_CheckStopAllActionsOnDemand(APluginReference) then
                begin
                  SetTemplateVar(CActionPlugin_ExecutionResultErrorVar, 'Plugin stopped on demand.');
                  Exit;
                end;

                if PluginSettings.ValueFromIndex[CParentFindControl_PropIndex] <> '' then
                  if not AOnActionPlugin_ExecuteAction(APluginReference, @PluginSettings.ValueFromIndex[CParentFindControl_PropIndex][1]) then  //call FindControl or CallTemplate, before each FindSubControl
                  begin
                    SetTemplateVar(CActionPlugin_ExecutionResultErrorVar, 'The required "' + PluginSettings.ValueFromIndex[CParentFindControl_PropIndex] + '" action has failed on: ' + s);
                    Exit;
                  end;

                if not AOnActionPlugin_ExecuteAction(APluginReference, @s[1]) then
                begin
                  //SetTemplateVar(APluginReference, CActionPlugin_ExecutionResultErrorVar, 'One of the required FindSubControl actions has failed: ' + s);
                  //Exit;
                end;

                GetAllTemplateVars(AllVarsStr);

                SetLength(AllFindSubControlExecutionResults_Corners, Length(AllFindSubControlExecutionResults_Corners) + 1);
                AllFindSubControlExecutionResults_Corners[Length(AllFindSubControlExecutionResults_Corners) - 1] := AllVarsStr;
              end;
        end;
      end;   //for  i := 0 to ActionCount - 1

      if Length(AllFindSubControlExecutionResults_Corners) <> 4 then   //expecting 4 actions
      begin
        SetTemplateVar(CActionPlugin_ExecutionResultErrorVar, 'Not all the required FindSubControl actions are found in the current template. Found ' +
                                                              IntToStr(Length(AllFindSubControlExecutionResults_Corners)) +
                                                              ' action(s).  PluginSettings: ' + PluginSettingsStr);

        DbgPoint('Invalid template', ResValue);

        Exit;
      end;

      //if not DbgPoint('Decoding windows', '') then
      //  Exit;

      //Actual window decoding:
      DecodeWindows(AllFindSubControlExecutionResults_Corners, WindowFeaturesArr);
      WindowFeaturesArrToString(WindowFeaturesArr, Control_Left, Control_Top, BorderThickness, DecodedWindowsResults);

      //Set results
      SetTemplateVar(CActionPlugin_ExecutionResultVar_XOffset, DecodedWindowsResults.ResultsX);
      SetTemplateVar(CActionPlugin_ExecutionResultVar_YOffset, DecodedWindowsResults.ResultsY);
      SetTemplateVar(CActionPlugin_ExecutionResultVar_Width, DecodedWindowsResults.ResultsW);
      SetTemplateVar(CActionPlugin_ExecutionResultVar_Height, DecodedWindowsResults.ResultsH);
      SetTemplateVar(CActionPlugin_ExecutionResultVar_Control_Lefts, DecodedWindowsResults.ControlLefts);
      SetTemplateVar(CActionPlugin_ExecutionResultVar_Control_Tops, DecodedWindowsResults.ControlTops);
      SetTemplateVar(CActionPlugin_ExecutionResultVar_Control_Rights, DecodedWindowsResults.ControlRights);
      SetTemplateVar(CActionPlugin_ExecutionResultVar_Control_Bottoms, DecodedWindowsResults.ControlBottoms);

      if MatchWindowEdges = 'True' then
      begin
        DbgPoint('Searching for window edges', 'start');

        SetLength(AllFindSubControlExecutionResults_Edges, 0);

        //Executing FindSubControl actions
        s := 'unset action';
        for i := 0 to ActionCount - 1 do
        begin
          GetActionInfoByIndex(i, s, ActionTypeDWord);

          if ActionTypeDWord <> $FFFFFFFF then
          begin
            ActionType := TClkAction(ActionTypeDWord);

            if ActionType in [acFindSubControl, acPlugin] then
              for j := CFindSubControlLeftEdge_PropIndex to CFindSubControlBottomEdge_PropIndex do    //edges:  without "- 1"
                if s = AllRequiredFindSubControlNames[j] then
                  for k := 0 to Length(WindowFeaturesArr) - 1 do  //Setting search area for current edge
                  begin
                    //if not DbgPoint('Calling FindControl or CallTemplate (edges)', PluginSettings.ValueFromIndex[CParentFindControl_PropIndex]) then
                    //  Exit;

                    if AOnActionPlugin_CheckStopAllActionsOnDemand(APluginReference) then
                    begin
                      SetTemplateVar(CActionPlugin_ExecutionResultErrorVar, 'Plugin stopped on demand.');
                      Exit;
                    end;

                    if PluginSettings.ValueFromIndex[CParentFindControl_PropIndex] <> '' then
                      if not AOnActionPlugin_ExecuteAction(APluginReference, @PluginSettings.ValueFromIndex[CParentFindControl_PropIndex][1]) then  //call FindControl or CallTemplate, before each FindSubControl
                      begin
                        SetTemplateVar(CActionPlugin_ExecutionResultErrorVar, 'The required "' + PluginSettings.ValueFromIndex[CParentFindControl_PropIndex] + '" action has failed on: ' + s);
                        Exit;
                      end;

                    //if not DbgPoint('Setting search area for current edge', 'Edge property index: ' + IntToStr(j) + '  Name: ' + s) then
                    //  Exit;

                    //DbgPoint('Matching edge', 'Current edge: ' + CEdgeNames[j] + '   Corner index: ' + IntToStr(k));

                    SetTemplateVar(CActionPlugin_EdgePropIndexVar, IntToStr(j));       //used later in DecodeWindowsByEdges
                    SetTemplateVar(CActionPlugin_WindowFeatureIndexVar, IntToStr(k));  //used later in DecodeWindowsByEdges
                    //Setting search area for every type of edges
                    case j of
                      CFindSubControlLeftEdge_PropIndex:
                      begin
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeLeftVar, IntToStr(WindowFeaturesArr[k].TopLeftCorner.X));
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeTopVar, IntToStr(WindowFeaturesArr[k].TopLeftCorner.Y + BorderThickness));
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeRightVar, IntToStr(WindowFeaturesArr[k].TopLeftCorner.X + BorderThickness));
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeBottomVar, IntToStr(WindowFeaturesArr[k].BotLeftCorner.Y + 1));
                        SetTemplateVar('$LE_Height$', IntToStr(WindowFeaturesArr[k].BotLeftCorner.Y - WindowFeaturesArr[k].TopLeftCorner.Y - BorderThickness - 1)); // "+ 1" gives a single result, but that is too tight
                      end;

                      CFindSubControlTopEdge_PropIndex:
                      begin
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeLeftVar, IntToStr(WindowFeaturesArr[k].TopLeftCorner.X + BorderThickness));
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeTopVar, IntToStr(WindowFeaturesArr[k].TopLeftCorner.Y));
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeRightVar, IntToStr(WindowFeaturesArr[k].TopRightCorner.X - 102));
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeBottomVar, IntToStr(WindowFeaturesArr[k].TopLeftCorner.Y + BorderThickness + 1));
                        SetTemplateVar('$TE_Width$', IntToStr(WindowFeaturesArr[k].TopRightCorner.X - WindowFeaturesArr[k].TopLeftCorner.X - BorderThickness - 102));
                      end;

                      CFindSubControlRightEdge_PropIndex:
                      begin
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeLeftVar, IntToStr(WindowFeaturesArr[k].TopRightCorner.X));
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeTopVar, IntToStr(WindowFeaturesArr[k].TopRightCorner.Y + BorderThickness));
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeRightVar, IntToStr(WindowFeaturesArr[k].TopRightCorner.X + BorderThickness));
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeBottomVar, IntToStr(WindowFeaturesArr[k].BotRightCorner.Y + 1));
                        SetTemplateVar('$LE_Height$', IntToStr(WindowFeaturesArr[k].BotLeftCorner.Y - WindowFeaturesArr[k].TopLeftCorner.Y - BorderThickness - 1));
                      end;

                      CFindSubControlBottomEdge_PropIndex:                   ///////////////////////// ToDo:  verify search area
                      begin
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeLeftVar, IntToStr(WindowFeaturesArr[k].BotLeftCorner.X + BorderThickness));
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeTopVar, IntToStr(WindowFeaturesArr[k].BotLeftCorner.Y - 1));
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeRightVar, IntToStr(WindowFeaturesArr[k].BotRightCorner.X));
                        SetTemplateVar(CActionPlugin_SearchArea_WinEdgeBottomVar, IntToStr(WindowFeaturesArr[k].BotLeftCorner.Y + BorderThickness));
                        SetTemplateVar('$TE_Width$', IntToStr(WindowFeaturesArr[k].BotRightCorner.X - WindowFeaturesArr[k].BotLeftCorner.X - BorderThickness));
                      end;
                    end;

                    //SetTemplateVar('$ResultedErrorCount$', '0');
                    //SetTemplateVar('$AllControl_ResultedErrorCount$', '0' + #4#5);
                    //SetTemplateVar('$AllControl_Lefts$', '');
                    //SetTemplateVar('$AllControl_Tops$', '');
                    //SetTemplateVar('$AllControl_Rights$', '');
                    //SetTemplateVar('$AllControl_Bottoms$', '');

                    if not AOnActionPlugin_ExecuteAction(APluginReference, @s[1]) then
                    begin
                      //SetTemplateVar(CActionPlugin_ExecutionResultErrorVar, 'One of the required FindSubControl actions has failed: ' + s);
                      //Exit;
                    end;

                    if not DbgPoint('After matching edge', 'Current edge: ' + CEdgeNames[j] + '   Corner index: ' + IntToStr(k)) then
                      Exit;

                    GetAllTemplateVars(AllVarsStr);

                    SetLength(AllFindSubControlExecutionResults_Edges, Length(AllFindSubControlExecutionResults_Edges) + 1);
                    AllFindSubControlExecutionResults_Edges[Length(AllFindSubControlExecutionResults_Edges) - 1] := AllVarsStr;

                    ListOfAllVars := TStringList.Create;
                    try
                      ListOfAllVars.Text := AllVarsStr;
                      DbgPoint('Edge matching status', '  ' + ListOfAllVars.Values['$ResultedErrorCount$'] + '   ' + ListOfAllVars.Values['$AllControl_ResultedErrorCount$'] + ' ========');
                      DbgPoint('Edge results ', 'Xoff: ' + ListOfAllVars.Values['$AllControl_XOffsets$'] + '   Yoff: ' + ListOfAllVars.Values['$AllControl_YOffsets$'] + '   L: ' + ListOfAllVars.Values['$AllControl_Lefts$'] + '   T: ' + ListOfAllVars.Values['$AllControl_Tops$'] + '   R: ' + ListOfAllVars.Values['$AllControl_Rights$'] + '   B: ' + ListOfAllVars.Values['$AllControl_Bottoms$'] + '   Src: ' + ListOfAllVars.Values['$AllControl_MatchSource$'] + '   WinFeatureIdx: ' + ListOfAllVars.Values[CActionPlugin_WindowFeatureIndexVar]);

                      case j of
                        CFindSubControlLeftEdge_PropIndex:
                          DbgPoint('ValidCornerByEdge.Left', BoolToStr(IsValidCornerPair(ListOfAllVars, WindowFeaturesArr[k].TopLeftCorner.X, k, '$AllControl_Lefts$'), 'True', 'False') + '  X:Y = ' + IntToStr(WindowFeaturesArr[k].TopLeftCorner.X) + ':' + IntToStr(WindowFeaturesArr[k].TopLeftCorner.Y));

                        CFindSubControlTopEdge_PropIndex:
                          DbgPoint('ValidCornerByEdge.Top', BoolToStr(IsValidCornerPair(ListOfAllVars, WindowFeaturesArr[k].TopLeftCorner.Y, k, '$AllControl_Tops$'), 'True', 'False') + '  X:Y = ' + IntToStr(WindowFeaturesArr[k].TopLeftCorner.X) + ':' + IntToStr(WindowFeaturesArr[k].TopLeftCorner.Y));

                        CFindSubControlRightEdge_PropIndex:
                          DbgPoint('ValidCornerByEdge.Right', BoolToStr(IsValidCornerPair(ListOfAllVars, WindowFeaturesArr[k].BotRightCorner.X, k, '$AllControl_Rights$'), 'True', 'False') + '  X:Y = ' + IntToStr(WindowFeaturesArr[k].BotRightCorner.X) + ':' + IntToStr(WindowFeaturesArr[k].BotRightCorner.Y));

                        CFindSubControlBottomEdge_PropIndex:
                          DbgPoint('ValidCornerByEdge.Bottom', BoolToStr(IsValidCornerPair(ListOfAllVars, WindowFeaturesArr[k].BotRightCorner.Y, k, '$AllControl_Bottoms$'), 'True', 'False') + '  X:Y = ' + IntToStr(WindowFeaturesArr[k].BotRightCorner.X) + ':' + IntToStr(WindowFeaturesArr[k].BotRightCorner.Y));
                      end;
                    finally
                      ListOfAllVars.Free;
                    end;
                  end; //for k
          end; //valid action type
        end;   //for  i := 0 to ActionCount - 1

        DbgPoint('Done searching for window edges', ' Found ' + IntToStr(Length(AllFindSubControlExecutionResults_Edges)) + ' result(s).');
        DecodeWindowsByEdges(AllFindSubControlExecutionResults_Edges, WindowFeaturesArr, WindowFeaturesWidthEdgesArr);
        WindowFeaturesArrToString(WindowFeaturesWidthEdgesArr, Control_Left, Control_Top, BorderThickness, DecodedWindowsResults);

        //Set results
        SetTemplateVar(CActionPlugin_ExecutionResultVar_XOffset_WE, DecodedWindowsResults.ResultsX);
        SetTemplateVar(CActionPlugin_ExecutionResultVar_YOffset_WE, DecodedWindowsResults.ResultsY);
        SetTemplateVar(CActionPlugin_ExecutionResultVar_Width_WE, DecodedWindowsResults.ResultsW);
        SetTemplateVar(CActionPlugin_ExecutionResultVar_Height_WE, DecodedWindowsResults.ResultsH);
        SetTemplateVar(CActionPlugin_ExecutionResultVar_Control_Lefts_WE, DecodedWindowsResults.ControlLefts);
        SetTemplateVar(CActionPlugin_ExecutionResultVar_Control_Tops_WE, DecodedWindowsResults.ControlTops);
        SetTemplateVar(CActionPlugin_ExecutionResultVar_Control_Rights_WE, DecodedWindowsResults.ControlRights);
        SetTemplateVar(CActionPlugin_ExecutionResultVar_Control_Bottoms_WE, DecodedWindowsResults.ControlBottoms);
      end; //MatchWindowEdges
    finally
      PluginSettings.Free;
    end;

    DbgPoint('Exiting plugin', 'done');

    Result := True;
    //GenerateSomeResultImg; //this will overwrite the result image on Debugging tab
  except
    on E: Exception do
    begin
      ResName := CActionPlugin_ExecutionResultErrorVar;
      ResValue := 'Plugin exception: "' + E.Message + '".';
      AOnActionPlugin_SetTemplateVar(APluginReference, @ResName[1], @ResValue[1]);
    end;
  end;

  try
    AddToLog('Done executing plugin. Result = ' + BoolToStr(Result, 'OK', 'Error'));
  except
  end;
end;


function IsValidCornerPair(AListOfAllVars: TStringList; ACornerCoord, ACornerIndex: Integer; AVarName: string): Boolean; //call with WindowFeaturesArr[k].TopLeftCorner.X
var
  ListOfMultiResult: TStringList;
begin
  Result := False;

  ListOfMultiResult := TStringList.Create;
  try
    ListOfMultiResult.Text := FastReplace_45ToReturn(AListOfAllVars.Values[AVarName]);
    if (ACornerIndex > -1) and (ACornerIndex < ListOfMultiResult.Count) then
      Result := True;
  finally
    ListOfMultiResult.Free;
  end;
end;


function EditProperty(APluginReference: Pointer;
                      AOnActionPlugin_InMemFS: TOnActionPlugin_InMemFS;
                      AOnActionPlugin_AppProcMsg: TOnActionPlugin_AppProcMsg;
                      APropertyIndex: Integer;
                      ACurrentValue: Pointer;
                      ANewValue: Pointer;
                      ALenOfNewValue: PDWord): Boolean; cdecl;
var
  CurrentValue, NewValue: string;
begin
  SetPointedContentToString(ACurrentValue, CurrentValue);

  NewValue := CurrentValue;

  //open editor here
  //Result := EditCustomFontProfilesProperty(APluginReference, AOnActionPlugin_InMemFS, APropertyIndex, CurrentValue, NewValue);
  ALenOfNewValue^ := SetPointedContentFromString(NewValue, ANewValue);
end;


