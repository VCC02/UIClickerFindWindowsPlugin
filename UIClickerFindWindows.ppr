          WindowFeaturesArr[Length(WindowFeaturesArr) - 1].TopLeftCorner.Y := TopLeftCorner_YOffsetsIntArr[j];
          WindowFeaturesArr[Length(WindowFeaturesArr) - 1].TopLeftCornerSrcIdx := TopLeftCorner_SrcIdx[j];
